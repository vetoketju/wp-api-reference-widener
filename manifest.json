{
  "name": "windowsphone API reference widener",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Modifies the body width to auto on the windows phone API reference pages. Made with extensionizr.io",
  "homepage_url": "http://msdn.microsoft.com/library/windowsphone/develop/ff626516.aspx",
  "default_locale": "en",
  "permissions": [
    "http://msdn.microsoft.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://msdn.microsoft.com/en-us/library/windowsphone/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}